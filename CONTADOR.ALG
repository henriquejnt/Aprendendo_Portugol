Algoritmo "CONTADOR"

Var
   contador,n1,sal:inteiro
Inicio
   contador <- 0
   escreva("Quer contar até quanto?")
   leia(n1)
   escreva("Qual será o valor do salto?")
   leia(sal)
   enquanto (contador <= n1)faça
      escreval(contador)
      contador <- contador + sal
   fimenquanto
   escreva("----------TERMINOU----------")




Fimalgoritmo